{
  "name": "nc-project-be",
  "version": "1.0.0",
  "description": "backend for final project of northcoders",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "seed:prod": "NODE_ENV=production DATABASE_URL=$(heroku config:get DATABASE_URL) npm run seed",
    "setup-dbs": "psql -f ./db/setup.sql",
    "seed": "node ./db/seeds/run-seed.js",
    "test": "jest",
    "prepare": "husky install && chmod ug+x .husky/*",
    "dev": "nodemon index.js",
    "build": "tsc",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/northcoders/be-nc-games.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/northcoders/be-nc-games/issues"
  },
  "homepage": "https://github.com/northcoders/be-nc-games#readme",
  "dependencies": {
    "18": "0.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "envalid": "^7.3.1",
    "express": "^4.18.2",
    "module-alias": "^2.2.2",
    "mongoose": "^6.6.5",
    "morgan": "^1.10.0",
    "node": "^18.8.0",
    "pg": "^8.7.3",
    "pg-format": "^1.0.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.8.3",
    "husky": "^7.0.0",
    "install": "^0.13.0",
    "jest": "^27.5.1",
    "jest-extended": "^2.0.0",
    "jest-sorted": "^1.0.14",
    "nodemon": "^2.0.20",
    "npm": "^8.19.2",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^4.8.4"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-extended/all",
      "jest-sorted"
    ]
  },
  "_moduleAliases": {
    "@/resources/*" : "dist/resources",
    "@/utils/*" : "dist/utils",
    "@/middleware/*" : "dist/middleware"
  }
}
